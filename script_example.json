{
  "server": "https://api-v5-dstu2.hspconsortium.org/InfernoDev/data",
  "arguments": {
    "client_name": "",
    "initiate_login_uri": "http://localhost:4568/launch",
    "redirect_uris": "http://localhost:4568/redirect",
    "scopes": "launch launch/patient offline_access openid profile user/*.* patient/*.*",
    "confidential_client": true,
    "standalone_launch_script": [
      {"cmd": "find_element", "type": "name", "value": "email"},
      {"cmd": "send", "value": "inferno@groups.mitre.org"},
      {"cmd": "find_element", "type": "name", "value": "password"},
      {"cmd": "send", "value": "temp_password"},
      {"cmd": "find_element", "type": "tag_name", "value": "button", "index": 3},
      {"cmd": "click"},
      {"cmd": "find_element", "type": "class", "value": "patient-row", "index": 0},
      {"cmd": "click"},
      {"cmd": "find_element", "type": "id", "value": "authorize"},
      {"cmd": "click"}
    ]
  },
  "sequences": [
    "Conformance",
    "DynamicRegistration",
    "TokenRefresh",
    "OpenIDConnect",
    "ArgonautPatient",
    {
      "sequence": "PatientStandaloneLaunch",
      "standalone_launch_script": [
        {"cmd": "send_keys", "type": "name", "find_value": "email", "value": "inferno@groups.mitre.org"},
        {"cmd": "send_keys", "type": "name", "find_value": "password", "value": "temp_password"},
        {"cmd": "click", "type": "tag_name", "find_value": "button", "index": 3},
        {"cmd": "click", "type": "class", "find_value": "patient-row", "index": 1},
        {"cmd": "click", "type": "id", "find_value": "authorize"}
      ]
    },
    "ArgonautPatient"
  ]
}
